{
	"name": "sinkparquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "synapsepurviewday-WorkspaceDefaultStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SchemaName": {
				"type": "string"
			},
			"TableName": {
				"type": "string"
			},
			"ContainerName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@concat(dataset().SchemaName,'\\',dataset().TableName)",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().ContainerName",
					"type": "Expression"
				}
			},
			"compressionCodec": "none"
		},
		"schema": []
	},
	"type": "Microsoft.Synapse/workspaces/datasets"
}